//Linear search
#include<stdio.h>
void main()
  {
        int array[20],n,x;
        printf("Enter the number of elements in an array : ");
        scanf("%d",&n);
        printf("\nEnter the array elements : ");
        for(int i=0;i<n;i++)
        {
            scanf("%d",&array[i]);
        }
   }
        
